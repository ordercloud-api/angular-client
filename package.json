{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@ordercloud/angular-sdk",
  "version": "1.0.0-4",
  "description": "angular 6.0.0+ typescript sdk for building on top of the ordercloud platform",
  "author": "ordercloud.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/ordercloud-api/angular-client"
  },
  "keywords": [
    "ordercloud",
    "four51",
    "b2b",
    "angular",
    "angular2",
    "ecommerce"
  ],
  "license": "MIT",
  "main": "dist/bundles/ordercloud-angular-sdk.umd.js",
  "module": "dist/esm5/ordercloud-angular-sdk.js",
  "es2015": "dist/esm2015/ordercloud-angular-sdk.js",
  "typings": "dist/ordercloud-angular-sdk.d.ts",
  "engines": {
    "node": ">=8.11.3",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
